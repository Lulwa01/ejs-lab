<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu</title>
</head>
<body>
  <%- include('./partials/nav') %>
  <h1>Full Menu</h1>
  <% locals.menuItems.forEach(element => { %>
   <li><%= element.name %>: <%= element.price %>: <%= element.rating %>: <%= element.details %></li>
  <% }) %>
</body>
</html>
